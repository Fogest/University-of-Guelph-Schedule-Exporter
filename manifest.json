{
  "manifest_version": 2,

  "name": "Extension Name",
  "version": "0.0.1",
  "description": "Extension Description",

  "permissions": [
    "https://webadvisor.uoguelph.ca/",
    "storage"
  ],

  "web_accessible_resources": ["script.js"],

  "content_scripts": [
    {
		"run_at": "document_end",
        "matches": ["https://webadvisor.uoguelph.ca/*"],
        "js": ["main.js","lib.js"],
		"css": ["style.css"],
        "all_frames": true
    }
  ]
}
